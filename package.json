{
  "name": "ifever-scanner",
  "description": "iFever Monitoring App",
  "version": "0.1.0",
  "author": {
    "name": "Junwoo Kim",
    "email": "junwoo.kim@gmail.com"
  },
  "dependencies": {
    "EventEmitter": "^1.0.0",
    "bluebird": "^3.3.5",
    "debug": "^2.2.0",
    "events": "^1.1.0",
    "noble-device": "^1.3.0"
  },
  "scripts": {
    "start": "export NODE_PATH=./node_modules:$NODE_PATH; export NODE_ENV=production; forever -a -l /dev/null start --uid=ifever ./ifever.js",
